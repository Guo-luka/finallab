#include <stdio.h>
#include <string.h>
int main(int args, char* argv[]) {
    if (args != 2) {
        printf("please use the kill in right format\n");
    } else {
	int i;
	int pid = 0;
        for (i = 0; argv[1][i] != '\0'; i++) {
		if (argv[1][i] <= '9' && argv[1][i] >= '0') {
			pid = pid * 10 + (argv[1][i] - '0');	
		} else {
			printf("valid usage");		
		}	
	}
	printf("%d ", pid);
        if (kill(pid) == -1) {
            printf("kill failed\n");
            return -1;
        }
        printf("process %d is successfully killed\n", pid);
    }
    return 0;
}
